<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:v="using:userinterface.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="userinterface.Views.HiddenProfileSettingsView">
  <StackPanel Orientation="Vertical">
    <v:NamedEditableFieldView FieldName="Rotation"/>
    <v:NamedEditableFieldView FieldName="L/R Ratio"/>
    <v:NamedEditableFieldView FieldName="U/D Ratio"/>
    <v:NamedEditableFieldView FieldName="Speed Cap"/>
    <v:NamedEditableFieldView FieldName="Angle Snapping"/>
  </StackPanel>
</UserControl>
